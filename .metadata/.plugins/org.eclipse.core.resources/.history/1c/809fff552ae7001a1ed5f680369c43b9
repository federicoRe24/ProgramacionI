#include <stdio.h>


int main() {

	int numero1;
	int numero2;
	int resultado;
	int resultadoScan;

	printf("Numero 1? ");
	scanf("%d", numero1); /* TIPOS int %d - float %f - char %c */

	resultadoScan = scanf("%d", &numero1); /*& para indicar la posicion de memoria de la variable --> puntero */
	printf("Resultado scan %d - Numero 1 es %d", resultadoScan, numero1);

   return 0;

}
